{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "ES6",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "stripInternal": true,
    "noUnusedLocals": true,
    "paths": {
      "webpack-dev-middleware": [
        "./stub.d.ts"
      ]
    }
  }
}
